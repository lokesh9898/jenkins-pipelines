pipeline {
    agent any
    stages {
        stage('Git pull Source Code') {
            steps {
                git branch: 'master', url: 'https://github.com/lokesh9898/jenkins-pipelines.git'
            }
        }
        stage ('check AWS'){
           steps {
                sh 'aws s3 ls'
           }
        }
    }
    post {
        always {
              cleanWs()
        }
    }
}
